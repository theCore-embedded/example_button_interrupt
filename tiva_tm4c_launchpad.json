{
    "menu-platform": {
        "config-name": "TM4C",
        "menu-tm4c": {
            "config-device": "TM4C123GH6PM",
            "config-console": "UART0",
            "menu-timer": {
                "config-source": "systick",
                "config-owner": "user",
                "config-freq": "100"
            },
            "menu-uart": {
                "table-uart": [
                    "UART0"
                ],
                "menu-UART0": {
